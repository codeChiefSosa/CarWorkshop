@{
   

    <table class="table">
        <thead class="thead-dark">
            <tr>
                <th scope="col">Brand</th>
                <th scope="col">Licence Plates</th>
                <th scope="col">Repair status</th>
            </tr>
        </thead>
        <tbody>
            @foreach (Car car in ViewData["CarCollection"] as IEnumerable<Car>)
            {
                <tr>
                    <td>@car.Brand</td>
                    <td>@car.Plates</td>
                    <td>@car.Repaired</td>
                </tr>
            }
        </tbody>
    </table>
}
